display: flex;\r\n    justify-content: space-between;    \r\n    img {\r\n        width: auto;\r\n        height: 10vh;\r\n    }\r\n    button {\r\n        padding-right: 2vw;\r\n        height: 10vh;\r\n        width: 20vw;\r\n    }\r\n}\r\n\r\n\r\n\r\n#book {\r\n    background: black;\r\n    margin: auto;\r\n    @media (min-width: 720px) {\r\n        margin: 10vh auto;\r\n    }\r\n    width: 95vw; \r\n    @media (min-width: 720px) {\r\n        width: 60vw;\r\n    }\r\n    height: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    padding: 1em;\r\n    form {\r\n        font-size: 12pt;\r\n        text-align: center;\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        flex-direction: space-evenly;\r\n        div {\r\n            width: 80vw;\r\n            @media (min-width: 720px) {\r\n                width: 45vw;\r\n            }\r\n        }\r\n        h3::after {\r\n            content: ' * ';\r\n            font-size: larger;\r\n            color: $green;\r\n        }\r\n        input{\r\n            font-family: 'Slabo 27px';\r\n            text-align: center;\r\n            height: 8vh;\r\n            padding: 2vw;\r\n            margin: 2vw;\r\n            width: 75vw;\r\n            @media (min-width: 720px) {\r\n                padding: 1vw;\r\n                margin: 1vw;\r\n                width: 40vw;\r\n            }\r\n            background: $white;\r\n        }\r\n        .invalid {\r\n            border: 2px solid red;\r\n        }\r\n        .error {\r\n            display: none;\r\n            color: red;\r\n        }\r\n        input[type=\"radio\"] {\r\n            opacity: 0;\r\n            height: 0;\r\n        }\r\n        .restaurant {\r\n            @include selectionBox();\r\n        }\r\n        .time {\r\n            @include selectionBox();\r\n            h3 {\r\n                height: 8vh;\r\n            }                  \r\n            #hour-selection {\r\n                span {\r\n                    background: $white;\r\n                    cursor: pointer;\r\n                    height: 8vh;\r\n                    margin: 2vh 0;\r\n                    display: block;\r\n                    color: black;\r\n                    &:hover {\r\n                        background: $green;\r\n                    }\r\n                }\r\n            }\r\n            #time-selection {\r\n                display: none;\r\n                position: relative;\r\n                button {\r\n                    width: 75vw;\r\n                    @media (min-width: 720px) {\r\n                        position: absolute;\r\n                        top: -9vh;\r\n                        right: 0;\r\n                        width: 7vw;\r\n                    }\r\n                }   \r\n            }    \r\n        }\r\n        .party {\r\n            @include selectionBox();\r\n        }\r\n        .message {\r\n            label {\r\n                h3:after {\r\n                    content: '';\r\n                }\r\n            }\r\n            textarea {\r\n            @extend input;\r\n            height: 30vh;\r\n            resize: vertical;\r\n            width: 75vw;\r\n            @media (min-width: 720px) {\r\n                width: 40vw;\r\n            }\r\n            font-family: 'Slabo 27px';\r\n            text-align: center;\r\n            background: $white;\r\n            color: black;\r\n            }\r\n        }\r\n        input[type=submit] {\r\n            @extend button;\r\n            width: 75vw;\r\n            @media (min-width: 720px) {\r\n                width: 8vw;\r\n            }\r\n        }\r\n        .submit-message {\r\n            color: $green;\r\n            text-align: center;\r\n            width: 75vw;\r\n            display: none;\r\n            opacity: 0;\r\n            transition: opacity 0.2s ease-in;\r\n            @media (min-width: 720px) {\r\n                width: 40vw;\r\n            }\r\n        }\r\n        .failure-message {\r\n            @extend .submit-message;\r\n        }\r\n    }\r\n}\r\n","styles\\_variables.scss":"$white: #F5F5F5;\r\n$green: #36970F;\r\n$darkgreen: #205909;\r\n\r\n@mixin flexCenter ($direction) {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: $direction\r\n}"},"lineCount":null}},"error":null,"hash":"dcfea404fb9b3306510e276019288d17","cacheData":{"env":{}}}